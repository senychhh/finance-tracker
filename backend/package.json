{
  "name": "dipl-backend", 
  "version": "1.0.0",
  "description": "Personal finance tracking API",
  "main": "dist/index.js", 
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts", 
    "build": "tsc", 
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts", 
    "test:db": "ts-node -r dotenv/config src/scripts/test-db.ts", 
    "db:schema": "ts-node -r dotenv/config src/scripts/apply-schema.ts"
  },
  "keywords": [
    "finance",
    "api",
    "express",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "@types/pg": "^8.10.9",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}